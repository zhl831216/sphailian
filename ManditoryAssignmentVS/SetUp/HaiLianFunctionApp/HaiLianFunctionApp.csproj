    <PackageReference Include="Blun.ConfigurationManager" Version="1.0.5686.2674" />
    <PackageReference Include="Microsoft.IdentityModel.Clients.ActiveDirectory" Version="3.16.1" />
    <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="1.0.4" />
    <PackageReference Include="SharePointPnPCoreOnline" Version="2.18.1709" />
